function setIMG() {
  var sheet = SpreadsheetApp.getActiveSpreadsheet().getSheetByName('xxx')/ / ชื่อชีตที่จะให้บันทึกลง
  var folder = DriveApp.getFolderById('xxxxx') // idForder
  var allFiles = folder.getFiles()
  var file
  while (allFiles.hasNext()) {
    file = allFiles.next()
    data = [
      file.getName(),
      "https://lh3.googleusercontent.com/d/" + file.getId(),
    ]
    sheet.appendRow(data)
  }
}
